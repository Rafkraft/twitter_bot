import hmac, hashlib, time

email = u"lol@lol.fr"
firstName = u"Yvesou"
lastName = u"duranty"
twitterUsername = u"lslls"
date1 = u'14'
date2 = u'01'
date3 = u'1910'
timestamp = u"1405432226"
secret_key = u"azkjbUBQYGV8UbV79BSscshz98JShssbqjpau4720sh7SSHSI76Bhsjsnm086"

to_compose = [email, firstName, lastName, twitterUsername, date1, date2, date3, timestamp]
hash = hmac.new(secret_key, ";".join(str(x) for x in to_compose), digestmod = hashlib.sha1)
message_auth = hash.hexdigest()
print message_auth


to_compose = [email, firstName, lastName, twitterUsername, date1, date2, date3, timestamp]
print to_compose
hash = hmac.new(secret_key, ";".join(str(x) for x in to_compose), digestmod = hashlib.sha1)
message_auth = hash.hexdigest()
print "message auth : "
print message_auth